  a owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  skos:prefLabel "drainage height in metres" ;
.
sprop:drainageHeightMethod
  a owl:ObjectProperty ;
  skos:prefLabel "Means of evaluation of drainage height" ;
.
sprop:elevation
