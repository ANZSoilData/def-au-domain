sprop:CoarseFragments
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://anzsoil.org/def/au/asls/land-surface/coarse-fragments-abundance> ;
      owl:onProperty sprop:coarseFragmentsAbundance ;
    ] ;
  skos:prefLabel "Coarse Fragments" ;
.
