.
sd:microrelief
  a owl:ObjectProperty ;
  skos:prefLabel "microrelief - zero, or choose one of the more specialized properties" ;
.
sd:modalSlope
  a owl:ObjectProperty ;
  skos:prefLabel "modal slope" ;
.
sd:morphologicalType
  a owl:ObjectProperty ;
  skos:prefLabel "morphological type" ;
.
sd:outcropAbundance
  a owl:ObjectProperty ;
