  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedProject ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSoilSite ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:purpose ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass [
          a owl:Class ;
          owl:unionOf (
              sosa:Observation
              sosa:Sampling
              ansis:Treatment
            ) ;
        ] ;
      owl:onProperty ansis:relatedActivity ;
    ] ;
  skos:example """my:visit-S1-2
  a ansis:SiteVisit ;
  prov:endedAtTime \"2022-09-07T13:00:00+10:00\"^^xsd:dateTime ;
  prov:startedAtTime \"2022-09-07T09:00:00+10:00\"^^xsd:dateTime ;
  ansis:relatedActivity my:O1-dens-A12-in-P1 ;
  ansis:relatedActivity my:O2-dens-S1-A12-in-P1 ;
  ansis:relatedActivity my:O3-dens-S2-L20-80-in-P1 ;
  ansis:relatedActivity my:O4-dens-S3-20-40-in-P1 ;
  ansis:relatedActivity my:O5-upperdepth-A12-in-P1 ;
  ansis:relatedProject <https://example.org/project-9765> ;
  ansis:relatedSite my:Site_1 ;
  ansis:visitor <https://orcid.org/0000-0002-0693-1899> ;
.
my:O1-dens-A12-in-P1
  a sosa:Observation ;
  skos:prefLabel \"Measurement of bulk density of a layer A12 in profile P1\" ;
  sosa:hasFeatureOfInterest my:A12-in-P1 ;
  sosa:hasSimpleResult \"1.4 g/cm3\" ;
  sosa:observedProperty ansis:density-bulk ;
  sosa:resultTime \"2022-09-14T11:35:00\"^^xsd:dateTime ;
.""" ;
  skos:prefLabel "Site visit" ;
.
ansis:SoilBody
  a owl:Class ;
  rdfs:subClassOf ansis:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSoilSurface ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilClassification ;
      owl:onProperty ansis:classification ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedLayer ;
    ] ;
  skos:definition """part of the soil cover that is delineated at a scale useful for an application, and is homogeneous with regard to properties and/or spatial patterns. 
""" ;
  skos:example """my:Body-88
  a ansis:SoilBody ;
  skos:prefLabel \"Monivae gravelly loam\" ;
.""" ;
  skos:note "A soil body is usually described as a sequence of layers or arrangement of soil horizons. This may be a single layer. " ;
  skos:prefLabel "Soil body" ;
.
ansis:SoilClassification
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Classifier-code ;
      owl:onProperty ansis:classification-greatgroup ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Classifier-code ;
      owl:onProperty ansis:classification-subgroup ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Depth-code ;
      owl:onProperty ansis:classification-family-depth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Gravel-code ;
      owl:onProperty ansis:classification-family-gravel ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Texture-code ;
      owl:onProperty ansis:classification-family-A1texture ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Texture-code ;
      owl:onProperty ansis:classification-family-Btexture ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asc:Thickness-code ;
      owl:onProperty ansis:classification-family-A1thickness ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange asc:Classifier-code ;
      owl:onProperty ansis:classification-suborder ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange asc:Order-code ;
      owl:onProperty ansis:classification-order ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:example """[
      a ansis:SoilClassification ;
      ansis:classification-order \"CH\" ;
      ansis:classification-suborder \"AB\" ;
]""" ;
  skos:prefLabel "Soil classification" ;
.
ansis:SoilConsistence
