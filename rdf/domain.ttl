    ] ;
  skos:prefLabel "R Horizon" ;
.
ansis:SoilLayer
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-lower-pos ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-upper-pos ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedCoarseFragments ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedMottles ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSegregations ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedVoids ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:structure ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass [
          a owl:Class ;
          owl:unionOf (
              asls:Field-texture-grade-mineral
              asls:Field-texture-organic
            ) ;
        ] ;
      owl:onProperty ansis:texture ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedBody ;
    ] ;
  skos:definition "Laterally extensive region within a soil body, approximately parallel with the surface." ;
  skos:example """my:Layer_20-80
  a ansis:SoilLayer ;
  skos:prefLabel \"layer 20-80 cm\" ;
  ansis:depth-lower \"80 cm\" ;
  ansis:depth-upper \"20 cm\" ;
  ansis:relatedBody my:Body-88 ;
.""" ;
  skos:note "A depth-defined layer could be considered to be a sample of a soil-body. " ;
  skos:prefLabel "Soil layer" ;
.
ansis:SoilOrder
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  skos:prefLabel "Soil order" ;
.
ansis:SoilPlasticity
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:type ;
    ] ;
  rdfs:subClassOf [
