.
sd:Layer
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  skos:prefLabel "General soil layer - covers both depth-defined and pedogenic-defined cases" ;
.
sd:Site
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      rdfs:comment "links to activities may be better routed through Site-Visits, rather than the site directly" ;
      owl:onProperty sd:relatedActivity ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              sosa:Sampling
              sosa:Observation
              sosa:ObservationCollection
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:annualRainfall ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:siteType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:state ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:describer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:airPhoto ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:mapSheet ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:locationWithinLandform ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasGeometry ;
      owl:someValuesFrom <http://www.opengis.net/ont/sf#Point> ;
    ] ;
  skos:prefLabel "Soil sampling and observation site" ;
.
sd:SiteVisit
  a owl:Class ;
  rdfs:subClassOf prov:Activity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:describer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:site ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sd:relatedActivity ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              sosa:Observation
              sosa:Sampling
              sosa:ObservationCollection
            ) ;
        ] ;
    ] ;
  skos:prefLabel "Site Visit" ;
.
sd:SoilProfile
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:horizonRDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:profileObservationType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sd:horizon ;
    ] ;
  skos:definition "A soil profile is a vertical section of a soil from the soil surface through all its horizons to parent material, other consolidated substrate material or selected depth in unconsolidated material. " ;
  skos:note "A soil profile is a local sample of the horizon-sequence. Importantly, it records the set of horizons present in the sample (i.e. at this location). Each horizon may then have a detailed description determned through a set of observations relating to the different characteristic properties. " ;
  skos:prefLabel "Soil profile" ;
.
sd:aggradation
  a owl:ObjectProperty ;
  skos:prefLabel "aggradation" ;
.
sd:airPhoto
  a owl:ObjectProperty ;
  skos:prefLabel "in air-photo" ;
.
sd:annualRainfall
  a owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  skos:prefLabel "annual rainfall in mm" ;
.
sd:aspect
  a owl:DatatypeProperty ;
  rdfs:range xsd:integer ;
  skos:prefLabel "aspect - bearing to nearest 10 deg" ;
.
sd:coarseFragmentsAbundance
  a owl:ObjectProperty ;
  skos:prefLabel "abundance of coarse fragments" ;
.
sd:coarseFragmentsLithology
  a owl:ObjectProperty ;
  skos:prefLabel "lithology of coarse fragments" ;
.
sd:coarseFragmentsShape
  a owl:ObjectProperty ;
  skos:prefLabel "shape of coarse fragments" ;
.
sd:coarseFragmentsSize
  a owl:ObjectProperty ;
  skos:prefLabel "size of coarse fragments" ;
.
sd:coarseFragmentsStrength
  a owl:ObjectProperty ;
  skos:prefLabel "strength of coarse fragments" ;
.
sd:colour
  a owl:ObjectProperty ;
  skos:prefLabel "Munsell colour" ;
.
sd:colourMoisture
  a owl:ObjectProperty ;
  skos:prefLabel "moisture status for colour description" ;
.
sd:componentLandformElement
  a owl:ObjectProperty ;
  skos:prefLabel "component landform element" ;
.
sd:depthToWater
  a owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  skos:definition """GIve depth to free water at the site of soil observation, in metres, either above or below the soil surface, excluding litter and living vegetation. Prefix the depth above or below the soil surface as follows: 

+  Above soil surface
-   Below soil surface

If the is no free water, records:

0   No free water""" ;
  skos:prefLabel "depth to free water - in metres" ;
.
sd:describer
  a owl:ObjectProperty ;
  rdfs:subPropertyOf dcterms:contributor ;
  rdfs:subPropertyOf prov:wasAttributedTo ;
  skos:prefLabel "person who described the entity" ;
.
sd:drainageHeight
  a owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  skos:prefLabel "drainage height in metres" ;
.
sd:drainageHeightMethod
  a owl:ObjectProperty ;
  skos:prefLabel "Means of evaluation of drainage height" ;
.
sd:elevation
  a owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  skos:prefLabel "elevation in metres above sea level" ;
.
sd:elevationMethod
  a owl:ObjectProperty ;
  skos:prefLabel "Means of evaluation of elevation" ;
.
sd:erosion
  a owl:ObjectProperty ;
  skos:prefLabel "erosion - choose more specialized property" ;
.
sd:geomorphologyAgent
  a owl:ObjectProperty ;
  skos:prefLabel "geomorphological agent" ;
.
sd:geomorphologyMode
  a owl:ObjectProperty ;
  skos:prefLabel "mode of geomorphological activity" ;
.
sd:geomorphologyStatus
  a owl:ObjectProperty ;
  skos:prefLabel "status of geomorphological activity" ;
.
sd:height
  a owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  skos:definition "difference in elevation between the upper and lower margins of the element, measured along any slope line. Height can mean different things and must be carefully defined. For crests, ridges and hillocks, define the upper margin as the point where the selected slope line coalesces with others to form the ridge line. For depressions, define the lower margin as the point where the selected slope line coalesces with others to form the course line. " ;
  skos:prefLabel "height or depth in metres" ;
.
sd:horizon
  a owl:ObjectProperty ;
