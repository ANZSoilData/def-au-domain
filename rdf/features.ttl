      owl:onProperty sprop:slopeValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:underlyingMaterial ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
