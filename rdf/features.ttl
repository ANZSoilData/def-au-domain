geo:Feature
  rdfs:subClassOf prov:Entity ;
.
<https://anzsoil.org/def/au/domain>
  a owl:Ontology ;
  dcterms:contributor <mailto:peter.wilson@csiro.au> ;
  dcterms:created "2022-07-21"^^xsd:date ;
  dcterms:creator <https://orcid.org/0000-0002-2991-2308> ;
  dcterms:creator <https://orcid.org/0000-0002-3884-3420> ;
  dcterms:description "A domain-model for Australian soil data - entities" ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  dcterms:modified "2022-08-23"^^xsd:date ;
  schema:codeRepository <https://github.com/ANZSoilData/def-au-domain> ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://www.opengis.net/ont/geosparql> ;
  owl:imports <http://www.opengis.net/ont/sf> ;
  owl:imports <http://www.w3.org/ns/prov-o#> ;
  owl:imports sosa: ;
.
soil:Body
  a owl:Class ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasLayer ;
    ] ;
  skos:definition """part of the soil cover that is delineated at a scale useful for an application, and is homogeneous with regard to properties and/or spatial patterns. 
""" ;
  skos:note "A soil body is usually described as a sequence of layers or arrangement of soil horizons. This may be a single layer. " ;
  skos:prefLabel "Soil body" ;
.
soil:Boundary
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:depth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasHorizon ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasHorizon ;
    ] ;
  skos:definition "Interface between two soil horizons" ;
  skos:prefLabel "Soil boundary" ;
.
soil:BoundaryInProfile
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasBoundary ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedProfile ;
    ] ;
  skos:definition "Soil boundary observed in a specific profile" ;
  skos:prefLabel "Boundary in profile" ;
.
soil:Class
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  skos:definition "Classifier (order, sub-order, etc) from some soil classification scheme" ;
  skos:prefLabel "Soil class" ;
.
soil:Horizon
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:Layer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:fieldTexture ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:lowerBoundary ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:upperBoundary ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:horizonClass ;
    ] ;
  skos:note "one classification per classification scheme" ;
  skos:prefLabel "Soil horizon" ;
.
soil:LandCover
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:LandscapeEntity ;
  skos:prefLabel "Land cover, including vegetation" ;
.
soil:LandManagement
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  skos:prefLabel "Land management" ;
.
soil:LandSurface
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:elevationMethod ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasMicrorelief ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:outcropAbundance ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:outcropLithology ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:siteDisturbance ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:erosion ;
    ] ;
  skos:prefLabel "Land surface" ;
.
soil:LandUse
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  skos:prefLabel "Land use" ;
.
soil:Landform
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty soil:hasLandformElement ;
      owl:someValuesFrom soil:LandformElement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty soil:hasLandformPattern ;
      owl:someValuesFrom soil:LandformPattern ;
    ] ;
  skos:prefLabel "Landform" ;
.
soil:LandformElement
  a owl:Class ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:geomorphologyMode ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:underlyingMaterial ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass skos:Concept ;
      owl:onProperty soil:landformElementName ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:prefLabel "Landform element (~40m)" ;
.
soil:LandformPattern
  a owl:Class ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:geomorphologyMode ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:geomorphologyStatus ;
    ] ;
  skos:prefLabel "Landform pattern (>600m)" ;
.
soil:LandscapeEntity
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  skos:prefLabel "Landscape entity" ;
.
soil:Layer
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:lowerDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:upperDepth ;
    ] ;
  skos:definition "General soil layer - this covers both depth-defined and compositionally-defined cases. " ;
  skos:note """A soil body may be described as a single layer. 

A depth-defined layer could be considered to sample a soil-body. 
However, a soil-horizon is _part-of_ a soil-body, but not a _sample_ in the usual sense because it does not represent the body as a whole. 
So the general Soil Layer class is not defined as a sub-class of Sample. """ ;
  skos:prefLabel "Soil layer" ;
.
soil:LayerInProfile
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasLayer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedProfile ;
    ] ;
  skos:definition "Soil layer observed in a specific profile" ;
  skos:prefLabel "Layer in profile" ;
.
soil:Microrelief
  a owl:Class ;
  skos:prefLabel "Microrelief" ;
.
soil:Outcrop
  a owl:Class ;
  rdfs:subClassOf soil:LandscapeEntity ;
  skos:prefLabel "Outcrop" ;
.
soil:Profile
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf sosa:Sample ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.opengis.net/ont/sf#Point> ;
      owl:onProperty geo:hasGeometry ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soil:Body ;
      owl:onProperty sosa:isSampleOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedSite ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:depthToRHorizon ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:depthToSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasBoundary ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasLayer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass soil:Profile-type ;
      owl:onProperty soil:profileType ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "A soil profile is a vertical section of a soil from the soil surface through all its horizons to parent material, other consolidated substrate material or selected depth in unconsolidated material. " ;
  skos:note "A soil profile is a local sample of the horizon-sequence. Importantly, it records the set of horizons present in the sample (i.e. at this location). Each horizon may then have a detailed description determned through a set of observations relating to the different characteristic properties. " ;
  skos:prefLabel "Soil profile" ;
.
soil:Profile-type
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  skos:definition "Type of soil observation profile - e.g. pit, auger bore, core, exposure" ;
  skos:prefLabel "Type of profile" ;
.
soil:R-horizon
  a owl:Class ;
  rdfs:subClassOf soil:Horizon ;
  skos:prefLabel "R-Horizon" ;
.
soil:Sample
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf sosa:Sample ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soil:Layer ;
      owl:onProperty sosa:isSampleOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:lowerDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:upperDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedSite ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedProfile ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedSample ;
    ] ;
  skos:note "A soil sample may be from a single profile, or may be a composite-sample from multiple profiles at a single site" ;
  skos:prefLabel "Soil sample" ;
.
soil:Site
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf sosa:Platform ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasLandManagement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasLandUse ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:siteDisturbance ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty geo:hasGeometry ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass soil:LandscapeEntity ;
      owl:onProperty sosa:isSampleOf ;
    ] ;
  skos:prefLabel "Site" ;
  skos:scopeNote "'Site' acts as a kind of platform, where sensors may be deployed. " ;
  skos:scopeNote "'Site' is a geospatial entity (feature) which has a geomtry (incl. location)" ;
  skos:scopeNote "'Site' may be considered as a kind of Sample, since it is selected to be representative of a landscape entity" ;
.
soil:SiteVisit
  a owl:Class ;
  rdfs:subClassOf prov:Activity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom prov:Person ;
      owl:onProperty soil:visitor ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty prov:endedAtTime ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty prov:startedAtTime ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedSite ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass [
          a owl:Class ;
          owl:unionOf (
              sosa:Observation
              sosa:Sampling
              soil:Treatment
            ) ;
        ] ;
      owl:onProperty soil:relatedActivity ;
    ] ;
  skos:prefLabel "Site visit" ;
.
soil:Substrate
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:lithology ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:strength ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:texture ;
    ] ;
  skos:prefLabel "Substrate" ;
.
soil:SubstrateInProfile
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:hasSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:relatedProfile ;
    ] ;
  skos:definition "Soil substrate observed in a specific profile" ;
  skos:prefLabel "Substrate in profile" ;
.
soil:Surface
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:Layer ;
  skos:definition "the layer with upper-depth = 0 and zero thickness" ;
  skos:prefLabel "Soil surface" ;
.
soil:Treatment
  a owl:Class ;
  rdfs:subClassOf sosa:Actuation ;
  skos:definition "Intervention or treatment" ;
  skos:prefLabel "Treatment" ;
.
soil:boundaryDistinctness
  a owl:ObjectProperty ;
  rdfs:domain soil:Boundary ;
  skos:prefLabel "boundary distinctness" ;
.
soil:boundaryShape
  a owl:ObjectProperty ;
  rdfs:domain soil:Boundary ;
  skos:prefLabel "boundary shape" ;
.
soil:component-landform-element
  a owl:ObjectProperty ;
  dcterms:identifier "https://anzsoil.org/def/au/property/component-landform-element"^^xsd:anyURI ;
  rdfs:range soil:LandformElement ;
  skos:prefLabel "component landform element" ;
.
soil:conditionWhenDry
  a owl:ObjectProperty ;
  rdfs:domain soil:Surface ;
  skos:prefLabel "condition when dry" ;
.
soil:depth
  a owl:ObjectProperty ;
  skos:definition "depth of entity" ;
  skos:prefLabel "depth" ;
.
soil:depthToRHorizon
  a owl:ObjectProperty ;
  skos:prefLabel "depth to R-horizon" ;
.
soil:depthToSubstrate
  a owl:ObjectProperty ;
  skos:prefLabel "depth to substrate" ;
.
soil:drainageHeightMethod
  a owl:ObjectProperty ;
  skos:definition "Means of evaluation of drainage height" ;
  skos:prefLabel "drainage height method" ;
.
soil:elevationMethod
  a owl:ObjectProperty ;
  skos:definition "Means of evaluation of elevation" ;
  skos:prefLabel "elevation method" ;
.
soil:erosion
  a owl:ObjectProperty ;
  skos:prefLabel "erosion" ;
.
soil:fieldTexture
  a owl:ObjectProperty ;
  skos:prefLabel "field texture" ;
.
soil:geomorphologyMode
  a owl:ObjectProperty ;
  skos:definition "mode of geomorphological activity" ;
  skos:prefLabel "geomorphology mode" ;
.
soil:geomorphologyStatus
  a owl:ObjectProperty ;
  skos:prefLabel "status of geomorphological activity" ;
.
soil:hasBoundary
  a owl:ObjectProperty ;
  rdfs:range soil:Boundary ;
  skos:definition "related boundary within a soil body or sample" ;
  skos:prefLabel "related boundary" ;
.
soil:hasHorizon
  a owl:ObjectProperty ;
  rdfs:range soil:Horizon ;
  rdfs:subPropertyOf soil:hasLayer ;
  skos:definition "related soil horizon within a soil body or sample, or associated with a contact" ;
  skos:prefLabel "related soil horizon" ;
.
soil:hasLandCover
  a owl:ObjectProperty ;
  rdfs:range soil:LandCover ;
  skos:definition "land cover around this entity" ;
  skos:prefLabel "related land cover" ;
.
soil:hasLandManagement
  a owl:ObjectProperty ;
  rdfs:range soil:LandManagement ;
  skos:definition "land management around this entity" ;
  skos:prefLabel "related land management" ;
.
soil:hasLandSurface
  a owl:ObjectProperty ;
  rdfs:range soil:LandSurface ;
  skos:definition "land surface at this entity" ;
  skos:prefLabel "related land surface" ;
.
soil:hasLandUse
  a owl:ObjectProperty ;
  rdfs:range soil:LandUse ;
  skos:definition "land use around this entity" ;
  skos:prefLabel "related land use" ;
.
soil:hasLandform
  a owl:ObjectProperty ;
  rdfs:range soil:Landform ;
  skos:definition "landform around this entity" ;
  skos:prefLabel "related landform" ;
.
soil:hasLandformElement
  a owl:ObjectProperty ;
  rdfs:range soil:LandformElement ;
  skos:definition "landform element in the vicinity of this entity (~40m)" ;
  skos:prefLabel "related landform element" ;
.
soil:hasLandformPattern
  a owl:ObjectProperty ;
  rdfs:range soil:LandformPattern ;
  skos:definition "landform pattern around this entity (>600m)" ;
  skos:prefLabel "related landform pattern" ;
.
soil:hasLayer
  a owl:ObjectProperty ;
  rdfs:range soil:Layer ;
  rdfs:subPropertyOf sosa:hasSample ;
  skos:definition "related soil layer within a soil body or sample" ;
  skos:prefLabel "related soil layer" ;
.
soil:hasMicrorelief
  a owl:ObjectProperty ;
  rdfs:range soil:Microrelief ;
  skos:definition "microrelief at the site" ;
  skos:prefLabel "related microrelief" ;
.
soil:hasOutcrop
  a owl:ObjectProperty ;
  rdfs:range soil:Outcrop ;
  skos:definition "outcrop at this entity" ;
  skos:prefLabel "related outcrop" ;
.
soil:hasSoilSurface
  a owl:ObjectProperty ;
  rdfs:range soil:Surface ;
  skos:definition "soil surface of this entity" ;
  skos:prefLabel "related soil surface" ;
.
soil:hasSubstrate
  a owl:ObjectProperty ;
  rdfs:range soil:Substrate ;
  skos:definition "substrate at this entity" ;
  skos:prefLabel "related substrate" ;
.
soil:horizonClass
  a owl:ObjectProperty ;
  rdfs:range soil:Class ;
  skos:definition "soil classification - order, sub-order, etc (name or notation)" ;
  skos:prefLabel "soil classification" ;
.
soil:landformElementName
  a owl:ObjectProperty ;
  skos:prefLabel "landform element name" ;
.
soil:lithology
  a owl:ObjectProperty ;
  skos:definition "lithological type of material" ;
  skos:prefLabel "lithological type" ;
.
soil:lowerBoundary
  a owl:ObjectProperty ;
  rdfs:subPropertyOf soil:hasBoundary ;
  skos:prefLabel "lower boundary" ;
.
soil:lowerDepth
  a owl:ObjectProperty ;
  skos:prefLabel "lower depth" ;
.
soil:mapSheet
  a owl:ObjectProperty ;
  skos:prefLabel "on map-sheet" ;
.
soil:outcropAbundance
  a owl:ObjectProperty ;
  skos:prefLabel "abundance of rock outcrop" ;
.
soil:outcropLithology
  a owl:ObjectProperty ;
  skos:definition "lithological type of rock outcrop" ;
  skos:prefLabel "lithology of outcrop" ;
.
soil:profileType
