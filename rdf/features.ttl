<https://anzsoil.org/def/au/feature>
  a owl:Ontology ;
  dcterms:contributor <https://orcid.org/0000-0002-2991-2308> ;
  dcterms:created "2022-07-21" ;
  dcterms:creator <https://orcid.org/0000-0002-3884-3420> ;
  dcterms:creator <mailto:peter.wilson@csiro.au> ;
  dcterms:description "A domain-model for Australian soil data - features" ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  dcterms:modified "2022-07-21" ;
  schema:codeRepository <https://github.com/ANZSoilData/def-au-domain> ;
  owl:imports dcterms: ;
  owl:imports <http://www.opengis.net/ont/geosparql> ;
