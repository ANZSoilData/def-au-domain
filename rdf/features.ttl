ansis:LandscapeEntity
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf prov:Entity ;
  skos:prefLabel "Landscape entity" ;
.
ansis:LandsurveySite
  a owl:Class ;
  rdfs:subClassOf sosa:Platform ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedProject ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:disturbance ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasLandCover ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasLandManagement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasLandSurface ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasLandUse ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasLandform ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asls:Site-drainage ;
      owl:onProperty ansis:drainage ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty geo:hasGeometry ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onDataRange ansis:SiteType ;
      owl:onProperty ansis:type ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sosa:isSampleOf ;
      owl:someValuesFrom ansis:LandscapeEntity ;
    ] ;
  skos:definition "A Site established to make observations of landscape entities" ;
  skos:prefLabel "Land survey site" ;
  skos:scopeNote "'Site' acts as a kind of platform, where sensors may be deployed. " ;
  skos:scopeNote "'Site' is a geospatial entity (feature) which has a geometry (incl. location)" ;
  skos:scopeNote "'Site' may be considered as a kind of Sample, since it is selected to be representative of a landscape entity" ;
  skos:scopeNote "More specific types of site may include (i) SoilSite (ii) TERN Monitoring Site (iii) Farmer site" ;
.
ansis:SiteVisit
  a owl:Class ;
  rdfs:subClassOf prov:Activity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom prov:Person ;
      owl:onProperty ansis:visitor ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty prov:endedAtTime ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty prov:startedAtTime ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedProject ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSite ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:purpose ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass [
          a owl:Class ;
          owl:unionOf (
              sosa:Observation
              sosa:Sampling
              ansis:Treatment
            ) ;
        ] ;
      owl:onProperty ansis:relatedActivity ;
    ] ;
  skos:example """my:visit-S1-2
  a ansis:SiteVisit ;
  prov:endedAtTime \"2022-09-07T13:00:00+10:00\"^^xsd:dateTime ;
  prov:startedAtTime \"2022-09-07T09:00:00+10:00\"^^xsd:dateTime ;
  ansis:relatedActivity my:O1-dens-A12-in-P1 ;
  ansis:relatedActivity my:O2-dens-S1-A12-in-P1 ;
  ansis:relatedActivity my:O3-dens-S2-L20-80-in-P1 ;
  ansis:relatedActivity my:O4-dens-S3-20-40-in-P1 ;
  ansis:relatedActivity my:O5-upperdepth-A12-in-P1 ;
  ansis:relatedProject <https://example.org/project-9765> ;
  ansis:relatedSite my:Site_1 ;
  ansis:visitor <https://orcid.org/0000-0002-0693-1899> ;
.
my:O1-dens-A12-in-P1
  a sosa:Observation ;
  skos:prefLabel \"Measurement of bulk density of a layer A12 in profile P1\" ;
  sosa:hasFeatureOfInterest my:A12-in-P1 ;
  sosa:hasSimpleResult \"1.4 g/cm3\" ;
  sosa:observedProperty ansis:density-bulk ;
  sosa:resultTime \"2022-09-14T11:35:00\"^^xsd:dateTime ;
.""" ;
  skos:prefLabel "Site visit" ;
.
ansis:SoilBody
  a owl:Class ;
  rdfs:subClassOf ansis:LandscapeEntity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilClassification ;
      owl:onProperty ansis:classification ;
    ] ;
  skos:definition """part of the soil cover that is delineated at a scale useful for an application, and is homogeneous with regard to properties and/or spatial patterns. 
""" ;
  skos:example """my:Body-88
  a ansis:SoilBody ;
  skos:prefLabel \"Monivae gravelly loam\" ;
.""" ;
  skos:note "A soil body is usually described as a sequence of layers or arrangement of soil horizons. This may be a single layer. " ;
  skos:prefLabel "Soil body" ;
.
ansis:SoilHorizon
  a owl:Class ;
  rdfs:subClassOf ansis:SoilLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass asls:Boundary-distinctness ;
      owl:onProperty ansis:distinctness ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass asls:Boundary-shape ;
      owl:onProperty ansis:shape ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass ansis:HorizonDesignation ;
      owl:onProperty ansis:designation ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "soil layer which is compositionally and/or structurally homogeneous, delineated by pedological boundaries " ;
  skos:example """my:Horizon_B22s
  a ansis:SoilHorizon ;
  skos:prefLabel \"Horizon B22s\" ;
  ansis:designation [
      ansis:designation-master \"B\" ;
      ansis:designation-subdivision \"22\" ;
      ansis:designation-suffix \"s\" ;
    ] ;
  ansis:relatedBody my:Body-88 ;
.""" ;
  skos:note "Soil horizons are designated A1, B2, R etc. " ;
  skos:prefLabel "Soil horizon" ;
.
ansis:SoilHorizonR
  a owl:Class ;
  rdfs:subClassOf ansis:SoilHorizon ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue [
          a ansis:HorizonDesignation ;
          ansis:designation-master "R" ;
        ] ;
      owl:onProperty ansis:designation ;
    ] ;
  skos:prefLabel "R Horizon" ;
.
ansis:SoilLayer
