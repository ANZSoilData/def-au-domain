    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-lower ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-upper ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasCoarseFragments ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasCracks ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasCutans ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasMottles ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasPans ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasPores ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasRoots ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasSegregations ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSoilBody ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilPlasticity ;
      owl:onProperty ansis:plasticity ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilStrength ;
      owl:onProperty ansis:strength ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilStructure ;
      owl:onProperty ansis:structure ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asls:Consistence-stickiness ;
      owl:onProperty ansis:stickiness ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asls:Fabric-type ;
      owl:onProperty ansis:fabric ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asls:Soil-permeability ;
      owl:onProperty ansis:permeability ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asls:Water-repellence-codes ;
      owl:onProperty ansis:waterRepellence ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange [
          a owl:Class ;
          owl:unionOf (
              asls:Field-texture-grade-mineral
              asls:Field-texture-organic
            ) ;
        ] ;
      owl:onProperty ansis:texture ;
    ] ;
  skos:definition """region within a soil body usually observed as a specified depth interval within a profile
""" ;
  skos:example """my:Layer_20-80
  a ansis:SoilLayer ;
  skos:prefLabel \"layer 20-80 cm\" ;
  ansis:depth-lower \"80 cm\" ;
  ansis:depth-upper \"20 cm\" ;
  ansis:relatedBody my:Body-88 ;
.""" ;
  skos:note "A depth-defined layer could be considered to be a sample of a soil-body. " ;
  skos:prefLabel "Soil layer" ;
.
ansis:SoilProfile
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf sosa:Sample ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ansis:SoilBody ;
      owl:onProperty sosa:isSampleOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-reason ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSite ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-r-horizon ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSoilSurface ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilClassification ;
      owl:onProperty ansis:classification ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange asls:Site-drainage ;
      owl:onProperty ansis:drainage ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedLayer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass sf:Point ;
      owl:onProperty geo:hasGeometry ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass asls:Profile-construction ;
      owl:onProperty sosa:usedProcedure ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange ansis:Profile-purpose ;
      owl:onProperty ansis:purpose ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "A soil profile is a vertical section of a soil from the soil surface through all its horizons to parent material, other consolidated substrate material or selected depth in unconsolidated material. " ;
  skos:example """my:Profile_1
  a ansis:SoilProfile ;
  geo:hasGeometry my:Point_1 ;
  skos:prefLabel \"profile 1\" ;
  sosa:isSampleOf my:Body-88 ;
  ansis:classification-order <http://anzsoil.org/def/au/asc/CH-AB> ;
  ansis:coarse-fragment-distribution <http://anzsoil.org/def/au/asls/soil-profile/coarse-fragments-distribution-R> ;
  ansis:depth-RHorizon \"106 cm\" ;
  ansis:depth-Substrate \"106 cm\" ;
  ansis:depth-lower \"106 cm\" ;
  ansis:depth-upper \"0 cm\" ;
  ansis:relatedHorizon my:Horizon_A11 ;
  ansis:relatedHorizon my:Horizon_A12 ;
  ansis:relatedHorizon my:Horizon_A2 ;
  ansis:relatedHorizon my:Horizon_B21s ;
  ansis:relatedHorizon my:Horizon_B22s ;
  ansis:relatedSite my:Site_1 ;
  ansis:type-profile <http://anzsoil.org/def/au/asls/soil-profile/observation-type-C> ;
.""" ;
  skos:note """A soil profile is a local sample of a soil body. Importantly, it records the set of horizons present at this location. 
Each horizon may then have a detailed description determined through a set of observations relating to the different characteristic properties. """ ;
  skos:note "If type=composite, then it is expected that there are 2::n related profiles whose type=single" ;
  skos:note "The geometry of a composite soil-profile is a polygon or set of points.  " ;
  skos:prefLabel "Soil profile" ;
.
ansis:SoilSample
  a owl:Class ;
  rdfs:subClassOf sosa:Sample ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom owl:Class ;
      owl:onProperty ansis:component ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              ansis:SoilLayer
              ansis:SoilHorizon
              ansis:SoilSurface
              ansis:SoilSample
            ) ;
        ] ;
      owl:onProperty sosa:isSampleOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:component ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-lower ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:depth-upper ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilSampleType ;
      owl:onProperty ansis:type ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedProfile ;
    ] ;
  skos:example """my:S3-20-40-in-P1
  a ansis:Sample ;
  skos:prefLabel \"Sample 3 from depth 20-40 in profile 1\" ;
  sosa:isResultOf my:Sg-20-40-in-P1 ;
  sosa:isSampleOf my:Body-88 ;
  ansis:depth-lower \"40 cm\" ;
  ansis:depth-upper \"20 cm\" ;
  ansis:relatedProfile my:Profile_1 ;
.
my:Sg-20-40-in-P1
  a sosa:Sampling ;
  sosa:hasFeatureOfInterest my:Profile_1 ;
  sosa:hasResult my:S3-20-40-in-P1 ;
  sosa:madeBySampler <mailto:peter.wilson@csiro.au> ;
  sosa:resultTime \"2002-09-19T13:35:00\"^^xsd:dateTime ;
  sosa:usedProcedure <https://example.org/soil-sampling-procedure-AVH> ;
  ansis:relatedProject <https://example.org/project-438> ;
.""" ;
  skos:note """A soil sample must be associated to a profile. 
In the case of a composite-sample, the 'virtual profile' is composed of multiple individual profiles at a single site or on a face. """ ;
  skos:prefLabel "Sample of soil or soil entity" ;
.
ansis:SoilSite
  a owl:Class ;
  rdfs:subClassOf ansis:LandsurveySite ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:hasSubstrate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ansis:relatedSoilSurface ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ansis:SoilBody ;
      owl:onProperty sosa:isSampleOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange ansis:Site-purpose ;
      owl:onProperty ansis:purpose ;
    ] ;
  rdfs:subClassOf [
