sfeat:Layer
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  skos:prefLabel "General soil layer - covers both depth-defined and pedogenic-defined cases" ;
.
sfeat:Profile
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:horizonRDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:profileObservationType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:horizon ;
    ] ;
  skos:definition "A soil profile is a vertical section of a soil from the soil surface through all its horizons to parent material, other consolidated substrate material or selected depth in unconsolidated material. " ;
  skos:note "A soil profile is a local sample of the horizon-sequence. Importantly, it records the set of horizons present in the sample (i.e. at this location). Each horizon may then have a detailed description determned through a set of observations relating to the different characteristic properties. " ;
  skos:prefLabel "Soil profile" ;
.
sfeat:Site
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      rdfs:comment "links to activities may be better routed through Site-Visits, rather than the site directly" ;
      owl:onProperty sprop:relatedActivity ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              sosa:Sampling
              sosa:Observation
              sosa:ObservationCollection
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:annualRainfall ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:siteType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:state ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:describer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:airPhoto ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:mapSheet ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:locationWithinLandform ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasGeometry ;
      owl:someValuesFrom <http://www.opengis.net/ont/sf#Point> ;
    ] ;
  skos:prefLabel "Soil sampling and observation site" ;
.
sfeat:SiteVisit
  a owl:Class ;
  rdfs:subClassOf prov:Activity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:describer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:site ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sprop:relatedActivity ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              sosa:Observation
              sosa:Sampling
              sosa:ObservationCollection
            ) ;
        ] ;
    ] ;
  skos:prefLabel "Site Visit" ;
.
