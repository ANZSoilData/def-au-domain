soil:Body
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass soil:Substrate ;
      owl:onProperty soil:lowerBound ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:layer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass soil:LandSurface ;
      owl:onProperty soil:upperBound ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:prefLabel "Soil Body" ;
.
soil:Horizon
  a owl:Class ;
  rdfs:subClassOf geo:Feature ;
  rdfs:subClassOf soil:Layer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:horizonName ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty sprop:colour ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty soil:fieldTexture ;
    ] ;
  skos:prefLabel "Soil horizon" ;
.
